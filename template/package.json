{
  "name": "<%= project.name.package %>",
  "version": "0.1.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run dev",
    "start:myIP": "webpack-dev-server --env.dev --open --env.myIP",
    "dev": "webpack-dev-server --env.dev --open",
    "build": "webpack",
    "dev-build": "webpack --env.dev",
    "clean-build": "rm -rf dist",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/js/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "author": {
    "name": "<%= user.name %>",
    "email": "<%= user.email %>"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= user.name %>/<%= project.name.package %>"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "html-webpack-plugin": "^3.2.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "portscanner": "^2.1.1",
    "prettier": "1.12.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-stylish": "^0.1.8",
    "webpack-merge": "^4.1.0"
  }
}
